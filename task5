import java.util.Scanner;

public class denis {
    public static void main(String[] args) {
//        Даны два числа. Если квадратный корень из второго числа меньше первого числа, 
//        то увеличить второе число в пять раз.
        
        System.out.println("Введите сначала число 'а', а затем введите число 'в'");
        Scanner sc = new Scanner(System.in);
        double a = sc.nextDouble();
        double b = sc.nextDouble();
        sc.close();
        double c = Math.sqrt(b);
        double d = b * 5;
        if (c < a) {
            System.out.print("Так как квадратный корень из числа 'b' меньше числа 'a', \n " +
                    "то число 'b' увеличено в пять раз и составляет = " + d);
        } else {
            System.out.print("Квадратный корень из числа b больше числа a" + d);
        }

        }


    }
