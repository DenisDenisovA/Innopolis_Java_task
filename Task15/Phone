

//Создайте класс Phone, который содержит переменные number, model и weight.
//        ● Создайте три экземпляра этого класса.
//        ● Выведите на консоль значения их переменных.
//        ● Добавьте в класс Phone методы: receiveCall, имеет один параметр – имя
//        звонящего. Выводит на консоль сообщение “Звонит {name}”. Метод
//        getNumber – возвращает номер телефона.
//        ● Вызовите эти методы для каждого из объектов.


package Demo;

public class Phone {

    private int number;
    private String model;
    private int weight;
    String name;
    int numberphone;


    public int getnumber() {
        return number;
    }

    public void setNumber(int number) {
        this.number = number;
    }
    public void receiveCall(){
        System.out.print("Звонит: "+name);
    }

    public String getmodel() {
        return model;
    }

    public void setModel(String model) {
        this.model = model;
    }

    public int getweight() {
        return weight;

    }

    public void setWeight(int weight) {
        this.weight = weight;
    }

    public void setNumberphone(int numberphone) {
        this.numberphone = numberphone;
    }

    public int getNumberphone() {
        return numberphone;
    }


    public Phone(int number, String model, int weight) {
        this.number = number;
        this.model = model;
        this.weight = weight;
    }

    public Phone() {
    }

    static int count = 0;


}
