import java.util.Scanner;

public class denis {
    public static void main(String[] args) {

//        Даны радиус круга и сторона квадрата. У какой фигуры площадь больше?
        System.out.println("Введите сначала радиус круга, а затем длину стороны квадрата");
        Scanner sc = new Scanner(System.in);
        double radius = sc.nextInt();
        double storona = sc.nextInt();
        sc.close();
        if ((Math.PI * Math.pow(radius, 2)) > Math.pow(storona, 2)) {
            System.out.print("Площадь круга больше");
        } else {
            System.out.print("Площадь квадрата больше");
        }

        }


}
